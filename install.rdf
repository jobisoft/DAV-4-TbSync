<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:id>dav4tbsync@jobisoft.de</em:id>
        <em:type>2</em:type> <!-- type=extension --> 
        <em:bootstrap>true</em:bootstrap>
        <em:unpack>false</em:unpack>
        <em:multiprocessCompatible>true</em:multiprocessCompatible>
        <em:name>Provider for CalDAV &amp; CardDAV</em:name>
        <em:version>0.10.4</em:version>
        <em:description>Add sync support for CalDAV &amp; CardDAV accounts to TbSync</em:description>
        <em:creator>John Bieling</em:creator>
        <em:homepageURL>https://github.com/jobisoft/DAV-4-TbSync</em:homepageURL>
        <em:iconURL>chrome://dav4tbsync/skin/sabredav32.png</em:iconURL>
        <em:translator>Ettore Atalan (de)</em:translator>
        <em:translator>John Bieling (de)</em:translator>
        <em:translator>John Bieling (en-US)</em:translator>
        <em:translator>Alessandro Menti (it)</em:translator>
        <em:contributor>Brad2014</em:contributor>
        <em:contributor>Nathan Gauër</em:contributor>
        <em:contributor>Alexandr Heymdall (vCard parser)</em:contributor>	
        <em:creator>John Bieling</em:creator>

        <em:targetApplication>
          <!-- Thunderbird -->
          <Description>
            <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
            <em:minVersion>50.0</em:minVersion>
            <em:maxVersion>61.*</em:maxVersion>
          </Description>
        </em:targetApplication>

        <em:localized>
          <Description>
            <em:locale>de</em:locale>
            <em:name>Provider für CalDAV &amp; CardDAV</em:name>
            <em:description>Erweitert TbSync und erlaubt die Synchronisation von CalDAV &amp; CardDAV Konten (Kontakte, Aufgaben und Kalender).</em:description>
            <em:translator>Ettore Atalan (de)</em:translator>
            <em:translator>John Bieling (de)</em:translator>
            <em:translator>John Bieling (en-US)</em:translator>
            <em:translator>Alessandro Menti (it)</em:translator>
            <em:contributor>Brad2014</em:contributor>
            <em:contributor>Nathan Gauër</em:contributor>
            <em:contributor>Alexandr Heymdall (vCard parser)</em:contributor>	
            <em:creator>John Bieling</em:creator>
          </Description>
        </em:localized>

        <em:localized>
          <Description>
            <em:locale>it</em:locale>
            <em:name>Provider CalDAV e CardDAV per TbSync</em:name>
            <em:description>Aggiunge il supporto per gli account CalDAV e CardDAV a TbSync</em:description>
            <em:translator>Ettore Atalan (de)</em:translator>
            <em:translator>John Bieling (de)</em:translator>
